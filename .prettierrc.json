{
	"tabWidth": 2,
	"singleQuote": false,
	"overrides": [
		{
			"files": "*.sol",
			"options": {
				"printWidth": 150,
				"tabWidth": 2,
				"useTabs": true,
				"singleQuote": false,
				"bracketSpacing": false,
				"explicitTypes": "always"
			}
		},
		{
			"files": ["*.yaml", "*.yml"],
			"options": {
				"useTabs": false,
				"tabWidth": 2
			}
		}
	]
}
